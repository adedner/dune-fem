problem=3
polorder=1
gridtype=YASPGRID

if ! make GRIDTYPE=${gridtype} DUNE_PROBLEM_DIM=2 POLORDER=${polorder} PROBLEM=${problem} clean dgtest;
then
echo "Something went wrong"
exit 1
fi
epsilonne=0.01
./dgtest grid2d.dgf 1 4 ${epsilonne} 1
mv eoc.tex ${gridtype}_pol${polorder}_p${problem}_eps${epsilonne}.tex
pdflatex ${gridtype}_pol${polorder}_p${problem}_eps${epsilonne}.tex
rm -f *.{aux,log}

exit 0


#  ALUGRID_TETRA         DUNE_PROBLEM_DIM 3
#  ALUGRID_HEXA          DUNE_PROBLEM_DIM 3
#  ALBERTAGRID           DUNE_PROBLEM_DIM 2,3
#  YASPGRID              DUNE_PROBLEM_DIM 2,3

