if [ ! -d TEXFILES ] ; then 
mkdir TEXFILES
fi

for problem in 2; do
	for polorder in 0 1 2; do
		for gridtype in ALBERTAGRID YASPGRID; do

			make GRIDTYPE=${gridtype} DUNE_PROBLEM_DIM=2 POLORDER=${polorder} PROBLEM=${problem} clean dgtest;
			for epsilonne in 0.01 0.001 0.0; do
				./dgtest grid2d.dgf 10 0 ${epsilonne};
				mv eoc.tex TEXFILES/${gridtype}_pol${polorder}_p${problem}_eps${epsilonne}.tex;
				cd TEXFILES
				pdflatex ${gridtype}_pol${polorder}_p${problem}_eps${epsilonne}.tex;
				rm -f *.{aux,log}
				cd ..
				
			done
		done
	done
done
exit 0


#  ALUGRID_TETRA         DUNE_PROBLEM_DIM 3
#  ALUGRID_HEXA          DUNE_PROBLEM_DIM 3
#  ALBERTAGRID           DUNE_PROBLEM_DIM 2,3
#  YASPGRID              DUNE_PROBLEM_DIM 2,3

