problem=4
polorder=2
gridtype=ALUGRID_SIMPLEX
epsilonne=1.0
grape=100
level=5
repeats=1
dimw=2


if ! make GRIDTYPE=${gridtype} GRIDDIM=${dimw} POLORDER=${polorder} PROBLEM=${problem} clean dgtest;
then
echo "Something went wrong"
exit 1
fi
./dgtest grid${dimw}d.dgf ${repeats} ${level} ${epsilonne} ${grape} 
#./dgtest grid${dimw}d.dgf.ALUgrid ${repeats} ${level} ${epsilonne} ${grape} 1.0
#./dgtest box${dimw}d.dgf.ALUgrid ${repeats} ${level} ${epsilonne} ${grape} 1.0
# mv eoc.tex ${gridtype}_pol${polorder}_p${problem}_eps${epsilonne}.tex
# pdflatex ${gridtype}_pol${polorder}_p${problem}_eps${epsilonne}.tex
rm -f *.{aux,log}

exit 0


